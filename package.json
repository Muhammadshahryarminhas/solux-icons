{
  "name": "@soluxicons/icons",
  "version": "0.2.0",
  "description": "Modern SVG icon library for React Native, React JS, and Next.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Muhammadshahryarminhas/solux-icons.git"
  },
  "homepage": "https://github.com/Muhammadshahryarminhas/solux-icons",
  "bugs": {
    "url": "https://github.com/Muhammadshahryarminhas/solux-icons/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "react-native": "./dist/native/index.js",
      "import": "./dist/web/index.js",
      "require": "./dist/web/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json && node scripts/copy-svgs.ts",
    "release": "npm run normalize:svgs && npm run generate:icons && npm run build",
    "generate:icons": "node scripts/generate-icons.ts",
    "normalize:svgs": "node scripts/normalize-svgs.ts",
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-native": ">=0.70",
    "react-native-svg": ">=13"
  },
  "devDependencies": {
    "@types/react": "^19.2.0",
    "typescript": "^5.8.3",
    "prettier": "2.8.8"
  },
  "engines": {
    "node": ">=20"
  }
}
